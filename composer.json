{
    "name": "wprorok/basic-phalcon",
    "description": "A Phalcon Framework project",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.28"
        },
        "preferred-install": "dist"
    },
    "require": {
        "php": ">=8.2",
        "phalcon/zephir": "^0.18.0",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^2.10",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "scripts": {
        "post-create-project-cmd": [
            "composer install"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
